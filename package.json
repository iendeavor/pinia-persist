{
  "name": "pinia-plugin-persistedstate-2",
  "version": "2.0.1",
  "sideEffects": false,
  "engines": {
    "node": "^14 || ^16 || >=18"
  },
  "description": "Persist and rehydrate your Pinia state between page reloads",
  "main": "dist/index.cjs.prod.js",
  "module": "dist/index.es.prod.mjs",
  "unpkg": "dist/index.iife.prod.js",
  "jsdelivr": "dist/index.iife.prod.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iendeavor/pinia-plugin-persistedstate-2.git"
  },
  "bugs": {
    "url": "https://github.com/iendeavor/pinia-plugin-persistedstate-2/issues"
  },
  "homepage": "https://github.com/iendeavor/pinia-plugin-persistedstate-2#readme",
  "author": "Ernest <iendeavor.github.io@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "vue",
    "pinia",
    "plugin",
    "persist",
    "rehydrate",
    "async",
    "localforage"
  ],
  "scripts": {
    "build": "rollup -c",
    "check-size": "size-limit",
    "lint": "prettier --check .",
    "lint:fix": "prettier --write .",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "release": "git fetch && pnpm exec standard-version",
    "test": "jest --watch",
    "test:run": "jest"
  },
  "dependencies": {
    "shvl": "^3.0.0"
  },
  "peerDependencies": {
    "pinia": "^2"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@babel/preset-env": "7.19.4",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "5.0.0",
    "@size-limit/preset-small-lib": "7.0.8",
    "@types/jest": "29.1.2",
    "@types/node": "16.11.65",
    "@vue/test-utils": "2.1.0",
    "change-case": "4.1.2",
    "husky": "7.0.4",
    "jest": "29.1.2",
    "jest-environment-jsdom": "29.1.2",
    "pinia": "2.0.23",
    "pinst": "2.1.6",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "rollup": "2.79.1",
    "rollup-plugin-terser": "7.0.2",
    "rollup-plugin-typescript2": "0.32.1",
    "size-limit": "7.0.8",
    "standard-version": "9.5.0",
    "ts-jest": "29.0.3",
    "typescript": "4.7.4",
    "vue": "3.2.40"
  }
}
